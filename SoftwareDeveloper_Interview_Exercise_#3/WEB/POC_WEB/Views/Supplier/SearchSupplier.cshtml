@model Entities.DTOs.CompanyDTO

@{
    ViewData["Title"] = "Search Supplier";
}



<table id="tblSupplierSearch" class="table table-striped table-bordered dt-responsive nowrap" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Telephone</th>
        </tr>
    </thead>
</table>

<!--Spinner Modal-->

<div class="modal" id="busyModal" data-backdrop="static"  data-keyboard="false">
    <div class="center" style="margin-left:45%;margin-top:20%">
        <img src="~/Images/loader.gif" />
    </div>
    <button type="button" style="visibility: hidden" class="btn btn-secondary" id="btnClose" data-dismiss="modal" />
</div>

@section scripts
    {

                            <script src="~/js/Views/SearchSupplier/Index.js?v=@DateTime.Now.Ticks.ToString()"></script>
}
